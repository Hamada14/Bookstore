	<queryString language="SQL">
		<![CDATA[SELECT BOOKS.BOOK_TITLE AS title, SUM(SHOPPING_ORDER_ITEMS.QUANTITY) as quantity, SUM(SHOPPING_ORDER_ITEMS.PRICE) as price
FROM BOOKS NATURAL JOIN SHOPPING_ORDER_ITEMS NATURAL JOIN SHOPPING_ORDER
WHERE SHOPPING_ORDER.CHECKOUT_TIME >= NOW() - interval 1 MONTH
GROUP BY title
ORDER BY price DESC;]]>
	</queryString>
