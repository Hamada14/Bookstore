	<queryString language="SQL">
		<![CDATA[SELECT BOOKS.BOOK_TITLE AS TITLE, SUM(SHOPPING_ORDER_ITEMS.QUANTITY) AS QUANTITY, SUM(SHOPPING_ORDER_ITEMS.PRICE) AS SALES_SUM
FROM BOOKS, SHOPPING_ORDER_ITEMS, SHOPPING_ORDER
WHERE BOOKS.BOOK_ISBN = SHOPPING_ORDER_ITEMS.BOOK_ISBN AND SHOPPING_ORDER_ITEMS.SHOPPING_ORDER_ID = SHOPPING_ORDER.SHOPPING_ORDER_ID AND SHOPPING_ORDER.CHECKOUT_TIME >= NOW() - interval 1 MONTH
GROUP BY BOOKS.BOOK_TITLE;]]>
	</queryString>
